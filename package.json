{
	"name": "frontendtest",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore src",
		"lintfix": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
		"format:write": "prettier .  --write",
		"format:check": "prettier .  --check",
		"prepare": "husky",
		"test:unit": "vitest",
		"testcoverage:unit": "vitest run --coverage"
	},
	"dependencies": {
		"normalize.css": "^8.0.1",
		"pinia": "^2.1.7",
		"vue": "^3.4.21"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/test-utils": "^2.4.5",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.24.0",
		"husky": "^9.0.11",
		"jsdom": "^24.0.0",
		"prettier": "3.2.5",
		"sass": "^1.74.1",
		"typescript": "^5.2.2",
		"vite": "^5.2.0",
		"vitest": "^1.5.0",
		"vue-eslint-parser": "^9.4.2",
		"vue-tsc": "^2.0.6"
	}
}
